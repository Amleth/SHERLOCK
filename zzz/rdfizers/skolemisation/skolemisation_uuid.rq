DELETE {
    GRAPH ?g {
    	?s ?p ?o .
    	?o ?p2 ?o2 .
    }
}
INSERT {
    GRAPH ?g {
    	?s ?p ?nouvelleUri .
    	?nouvelleUri ?p2 ?o2 .
    }
}
WHERE {
    GRAPH ?g {
        ?s ?p ?o .
        ?o ?p2 ?o2 .
        # seulement les cas o√π l'objet est un noeud anonyme
        FILTER(isBlank(?o))
    }
        
    BIND(IRI(
            CONCAT("http://data-iremus.huma-num.fr/id/", STRUUID())
        ) 
        AS ?nouvelleUri
    )
}

SELECT *
WHERE {
  GRAPH <http://data-iremus.huma-num.fr/graph/polymir> {
    <http://data-iremus.huma-num.fr/id/1269f40f-a960-4c94-aa8d-bff42414ad89> ?p ?o .
  }
}

